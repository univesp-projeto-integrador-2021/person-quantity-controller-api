{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "class-methods-use-this":"off",
        "quotes": "off",
        "prefer-default-export": "off",
        "import/prefer-default-export": "off",
        "no-useless-constructor":"off",
        "no-return-await": "off",
        "no-empty-function": "off"
    }
}
